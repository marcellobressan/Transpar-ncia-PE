# Environment Variables Example
# Copy this file to .env and fill in your values

# Neon PostgreSQL Database URL
VITE_DATABASE_URL=postgresql://neondb_owner:YOUR_PASSWORD@YOUR_HOST.neon.tech/neondb?sslmode=require

# Gemini API Key (optional)
GEMINI_API_KEY=your_gemini_api_key_here
# Google API Key (for Fact Check and Custom Search APIs)
VITE_GOOGLE_API_KEY=your_google_api_key_here

# Google Custom Search Engine ID (optional, has default)
VITE_GOOGLE_CSE_ID=your_custom_search_engine_id